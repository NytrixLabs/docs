{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Nytrix Labs",
  "colors": {
    "primary": "#130f0f",
    "light": "#ffffff",
    "dark": "#000000"
  },
  "favicon": "/images/Frame_211.png",
  "navigation": {
    "pages": [
      "home"
    ]
  },
  "logo": "/images/Frame_211-removebg-preview.png",
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:nytrixlabs@proton.me"
      }
    ]
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/NytrixLabs"
    }
  },
  "appearance": {
    "strict": true,
    "default": "dark"
  },
  "background": {
    "decoration": "gradient",
    "color": {
      "dark": "#000000",
      "light": "#ffffff"
    }
  },
  "fonts": {
    "family": "Inter"
  },
  "banner": {
    "content": "Severn for Fluxer is currently **down**."
  },
  "thumbnails": {
    "background": "/images/1920x910-Blog-Header-1-4-1.jpg",
    "appearance": "dark"
  },
  "styling": {
    "latex": true
  },
  "metadata": {
    "timestamp": true
  },
  "interaction": {
    "drilldown": true
  },
  "seo": {
    "indexing": "all"
  },
  "errors": {
    "404": {
      "title": "You have entered the void.",
      "description": "You don't want to be in the void, so please click one of the links below."
    }
  }
}
